# サル
kd:baboon a kd:animal_of_India ;
  kgc:isFrom kd:India;
  kgc:hasProperty kd:no_poisonous.

# ゾウ
kd:elephant a kgc:animal_of_India ;
  kgc:isFrom kd:India;
  kgc:hasProperty kd:no_poisonous.

