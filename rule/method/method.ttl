PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
IF {
   ?x1 kgc:isnot kd:rightuse.
   ?x2 kgc:isnot kd:rightuse.
   kd:225 kgc:subject kd:ventilator;
          kgc:when kd:at_the_same_time_as_rope_of_bell_put.
   filter(?x1!=?x2)
   } THEN { 
    ?x1 kgc:sameOppose ?x2.
   } 
""". 

